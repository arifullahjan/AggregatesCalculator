        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NustEngineering"
            android:label="NUST"/>
        <activity android:name=".Fast" />
        <activity android:name=".Giki"
            android:label="Equation Solver" />
        <activity android:name=".Nums"
            android:label="NUMS"/>
        <activity android:name=".Etea"
            android:label="ETEA"/>
        <activity android:name=".FastNUTestScoreCalculator"
            android:label="NU Test Scorer"/>
        <activity android:name=".Uet"
            android:label="UET"/>
    </application>

</manifest>